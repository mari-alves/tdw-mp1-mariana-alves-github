name: work pipeline

on:
  push:
    branches: [ main ]

jobs:
  install:
    runs-on: ubuntu-latest

  strategy:
    matrix:
      node-version: "18.x"

  steps:
    - uses: actions/checkout@v2
    - name: Use Node.js
      uses: actions/setup-node@v2
      with:
        node-version: '12.x'
    - name: Install dependencies
      run: npm install

